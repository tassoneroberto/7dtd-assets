    <entity_class name="zombie04">
   		
    	<!-- This one decides for scoring -->
        <property name="EntityType" value="Zombie" />
        
        <!-- Mesh and class properties -->
        <property name="Mesh"   value="Zombies/zombie04Ragdoll" />
