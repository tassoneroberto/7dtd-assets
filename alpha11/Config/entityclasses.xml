        <property name="AvatarController" value="AvatarZombieController" />
        
        <!-- AI properties -->
        <property name="AITask-1" value="BreakDoor" />
        <property name="AITask-2" value="BreakBlock" />
        <property name="AITask-3" value="Territorial" param1="10" />
